@model IEnumerable<JobSocialPoster.Core.Models.ProfileCategory>

@{
    ViewBag.Title = "Kategorie profili";
}

<h2>Kategorie profili</h2>

<p>
    @Html.ActionLink("Utwórz nową kategorię", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>
    

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("Edycja", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Usuń", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
